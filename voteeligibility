#include <iostream>
using namespace std;

int main()
{
    int age;
    cout<<"Enter your age: ";
    cin>>age;
    
    if(age>=18)
    {
        cout<<"you are eligible to vote";
    }
    else if(age<18 && age>0)
    {
        cout<<"you are ineligible to vote";
    }
    else
    {
        cout<<"invalid input";
    }
    
    return 0;
}
